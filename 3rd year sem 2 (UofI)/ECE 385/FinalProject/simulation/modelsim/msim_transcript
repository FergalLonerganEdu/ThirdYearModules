vsim work.color_mapper
# vsim -gui "+altera" -l msim_transcript -do "Final_run_msim_rtl_verilog.do" 
# Start time: 15:19:54 on Apr 28,2016
# Loading sv_std.std
# Loading work.color_mapper
# Loading work.conor_rom
# Loading work.debjit_rom
# Loading work.sprite_addr_rom
wave create -driver freeze -pattern constant -value 010000000000000000 -range 17 0 -starttime 0ps -endtime 1000ps sim:/color_mapper/board
# Editable Wave items are not compatible with the wave window's standard undo-redo.  Standard undo-redo has been disabled.
wave create -driver freeze -pattern constant -value 0000110000 -range 9 0 -starttime 0ps -endtime 1000ps sim:/color_mapper/DrawX
wave create -driver freeze -pattern constant -value 0000110000 -range 9 0 -starttime 0ps -endtime 1000ps sim:/color_mapper/DrawY
add wave -position insertpoint  \
sim:/color_mapper/Red \
sim:/color_mapper/Green \
sim:/color_mapper/Blue
restart
# Loading sv_std.std
# Loading work.color_mapper
# Loading work.conor_rom
# Loading work.debjit_rom
# Loading work.sprite_addr_rom
run -all
wave modify -driver freeze -pattern constant -value 0000111111 -range 9 0 -starttime 0ps -endtime 1000ps Edit:/color_mapper/DrawX
wave modify -driver freeze -pattern constant -value 0000111111 -range 9 0 -starttime 0ps -endtime 1000ps Edit:/color_mapper/DrawY
restart
# Loading sv_std.std
# Loading work.color_mapper
# Loading work.conor_rom
# Loading work.debjit_rom
# Loading work.sprite_addr_rom
run -all
add wave -position insertpoint  \
sim:/color_mapper/shape1_on
restart
# Loading sv_std.std
# Loading work.color_mapper
# Loading work.conor_rom
# Loading work.debjit_rom
# Loading work.sprite_addr_rom
run -all
add wave -position insertpoint  \
sim:/color_mapper/debjit_data
restart
# Loading sv_std.std
# Loading work.color_mapper
# Loading work.conor_rom
# Loading work.debjit_rom
# Loading work.sprite_addr_rom
run -all
# End time: 15:24:06 on Apr 28,2016, Elapsed time: 0:04:12
# Errors: 0, Warnings: 0
